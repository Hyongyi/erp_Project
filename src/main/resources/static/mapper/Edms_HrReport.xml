<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.choongang.erpproject.edms.Repository.Hr_Mapper">

    <select id="levCount" resultType="int">
        SELECT count(lev_num) as cnt from lev_table;
    </select>

    <select id="findAll" resultType="com.choongang.erpproject.edms.dto.Hr_ReportDto">
        SELECT * FROM lev_table;
    </select>


</mapper>
